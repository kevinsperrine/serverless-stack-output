{
  "name": "@kevinsperrine/serverless-stack-output",
  "version": "0.2.6",
  "description": "Serverless plugin to process AWS CloudFormation Stack Output",
  "license": "MIT",
  "author": "Kevin S. Perrine <kperrine@gmail.com>",
  "main": "dist",
  "scripts": {
    "clean": "rimraf dist",
    "test": "jest",
    "test:cover": "jest --coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint": "tslint {src,test}/**/*.ts",
    "prebuild": "yarn clean",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sbstjn/serverless-stack-output.git"
  },
  "bugs": {
    "url": "https://github.com/sbstjn/serverless-stack-output/issues"
  },
  "homepage": "https://github.com/sbstjn/serverless-stack-output#readme",
  "keywords": [
    "serverless",
    "plugin",
    "aws",
    "cloudformation",
    "stack",
    "output"
  ],
  "dependencies": {
    "tomlify-j0.4": "^3.0.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/jasmine-data-provider": "^2.2.1",
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.12",
    "@types/sinon": "^5.0.7",
    "@types/yamljs": "^0.2.30",
    "coveralls": "^3.0.2",
    "dot-json": "^1.0.4",
    "husky": "^1.2.0",
    "jasmine-data-provider": "^2.2.0",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "rimraf": "^2.6.2",
    "sinon": "^7.2.0",
    "ts-jest": "^23.10.5",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.2"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    },
    "transform": {
      ".*": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write --single-quote --parser typescript",
      "tslint --project tsconfig.json --config tslint.json --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test"
    }
  }
}
